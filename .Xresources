*color15: #FFF

*background: #111
*foreground: #AAA

URxvt.font: xft:Terminus:bold:pixelsize=16
! URxvt.font: -*-proggyclean-*-*-*-*-*-*-*-*-*-*-*-*,7x13
! URxvt.font: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*,7x13
URxvt.scrollBar: off
URxvt.perl-ext:      default,url-select
URxvt.keysym.M-u:    perl:url-select:select_next
URxvt.url-select.launcher: google-chrome --incognito
URxvt.url-select.underline: true
URxvt*termName:rxvt
URxvt*saveLines: 32000
